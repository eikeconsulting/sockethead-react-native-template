{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "target": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "jsx": "react-native",
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@app/*": [
        "*"
      ],
    }
  }
}